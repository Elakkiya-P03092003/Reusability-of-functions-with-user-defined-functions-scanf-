#include <stdio.h>
 int rectangular(int l,int b){
       int c;
       c = l*b;
      return c;
 }
int main(){
    int l,b;
    scanf("%d %d",&l,&b);
    int c = rectangular(l,b);
    printf("%d\n",c);
    scanf("%d %d",&l,&b);
    c=rectangular(l,b);
    printf("%d",c);
    scanf("%d %d",&l,&b);
    c=rectangular(l,b);
    printf("%d",c);
    return 0;
}
